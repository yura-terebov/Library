package database;

public class User {
	private int id;
	private String name;
	private String author;
	
	public User() {
		
	}
	
	public User(String name, String author) {
		this.name = name;
		this.author = author;
	}
	
	public User(int id, String name, String author) {
		this.id = id;
		this.name = name;
		this.author = author;		
	}
}
